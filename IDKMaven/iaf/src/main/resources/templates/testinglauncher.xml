  @Before
  public void agentSetup() throws StaleProxyException, TimeOut{
  
  
        IAFProperties.setGraphicsOn(false); // disable graphics
         MainInteractionManager.goManual(); // Stop task execution
         
         new Thread(){
			public void run(){
				String[] args1=new String[4];
				args1[0]="-port";
