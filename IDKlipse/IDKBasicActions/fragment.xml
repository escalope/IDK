<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<fragment>
   <extension
         point="org.eclipse.ui.actionSets">
 <actionSet
            label="IDK Menu Bar Action Set"
            visible="true"
            id="idkactions">
         <menu
               label="IDK Menu Bar"
               id="sampleMenu">
            <separator
                  name="idkGroup">
            </separator>
         </menu>
         <action
               label="IAF Generate Code"
               icon="icons/script_go.png"
               class="ingenias.plugin.actions.IAFAction"
               tooltip="IAF Generate Code"
               menubarPath="sampleMenu/idkGroup"
               toolbarPath="idkGroup"
               id="p.actions.IAFAction">
         </action>
         <action
               class="ingenias.plugin.actions.AppLinkerAction"
               icon="icons/table_refresh.png"
               id="ingenias.plugin.actions.IAFAction"
               label="AppLinker"
               menubarPath="sampleMenu/idkGroup"
               toolbarPath="idkGroup"
               tooltip="AppLinker">
         </action>
         <action
               class="ingenias.plugin.actions.HTMLAction"
               icon="icons/html_go.png"
               id="ingenias.plugin.actions.IAFAction"
               label="Generate HTML Documentation"
               menubarPath="sampleMenu/idkGroup"
               toolbarPath="idkGroup"
               tooltip="Generate HTML Documentation">
         </action>
         <action
               class="ingenias.plugin.actions.UploaderAction"
               icon="icons/database_refresh.png"
               id="ingenias.plugin.actions.IAFAction"
               label="Code Uploader"
               menubarPath="sampleMenu/idkGroup"
               toolbarPath="idkGroup"
               tooltip="Code Uploader">
         </action>
     
      </actionSet>
   </extension>

</fragment>
